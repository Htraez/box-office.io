<div class="container-fluid">
    <div class="container" >
        <div class="form-group row justify-content-md-center">
            <label for="date"  class="col-md-2 col-sm-2 col-3 mt-2 mb-0">Date</label>
            <div class="row align-items-start">
                <div class="col">
                <input type="date" autocomplete="off" class="form-control text-white pb-0 text-center" id="date" name="date">
                </div>
            </div>
        </div>

        <div class="form-group row justify-content-md-center">
            <label for="startTime" class="col-md-2 col-sm-2 col-3 mt-2 mb-0">Start time</label>
            <div class="row align-items-start">
                <div class="col">
                <input type="number" autocomplete="off" class="form-control text-white pb-0 text-center" id="HHstartTime"  name="HHstartTime" style=" width: 50px;" > 
                </div>  
                <div class="col">
                <input type="number" autocomplete="off" class="form-control text-white pb-0 text-center" id="MMstartTime"  name="MMstartTime" style=" width: 50px;">
                </div>
                <div class="col">
                <input type="number" autocomplete="off" class="form-control text-white pb-0 text-center" id="SSstartTime"  name="SSstartTime" style=" width: 50px;">
                </div>
            </div>
        </div>

        <div class="form-group row justify-content-md-center">
            <label for="endTime" class="col-md-2 col-sm-2 col-3 mt-2 mb-0">End time</label>
            <div class="row align-items-start">
                <div class="col">
                <input type="number" autocomplete="off" class="form-control text-white pb-0 text-center" id="HHendTime"  name="HHendTime" style=" width: 50px;" > 
                </div>  
                <div class="col">
                <input type="number" autocomplete="off" class="form-control text-white pb-0 text-center" id="MMendTime"  name="MMendTime" style=" width: 50px;">
                </div>
                <div class="col">
                <input type="number" autocomplete="off" class="form-control text-white pb-0 text-center" id="SSendTime"  name="SSendTime" style=" width: 50px;">
                </div>
            </div>
        </div>
        
        <div class="row justify-content-around" style="padding-top: 50px; width: 780px;">
            <button type="button" id="back" class="btn btn-white btn-outline-light ">Back</button>
            <button type="button" id="assignShift" class="btn btn-white btn-outline-light ">Assign</button>
        </div>
        <br>
        <div class="row justify-content-md-center" style="padding-top: 50px; width: 820px;">
            <div class="row align-items-start">
                <div class="col">
                <button type="button" id="savedata" class="btn btn-white mr-5 btn-outline-light" >Save</button>
                </div>

            </div>  
    </div>
</div>

